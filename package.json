{
    "name": "dotlang",
    "displayName": "dotlang",
    "description": "Add a syntax hightlight support to dotlang files",
    "version": "1.0.0",
    "publisher": "Nobuwu",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/NobUwU/dotlang"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lang",
                "aliases": [
                    "dotlang",
                    "lang"
                ],
                "extensions": [
                    ".lang"
                ]
            }
        ],
        "grammars": [
            {
                "language": "lang",
                "scopeName": "source.lang",
                "path": "./syntaxes/lang.tmLanguage.json"
            }
        ]
    }
}
